# Appwrite Configuration
# Copy this file to .env and fill in your Appwrite project details

# Appwrite project ID (from your Appwrite console)
VITE_APPWRITE_PROJECT_ID=your-project-id-here

# Appwrite project name (optional, for reference)
VITE_APPWRITE_PROJECT_NAME=Browser-usage-tracker

# Appwrite server endpoint (use regional endpoint for better performance)
VITE_APPWRITE_ENDPOINT=https://cloud.appwrite.io/v1

# Your Appwrite database ID
VITE_APPWRITE_DATABASE_ID=usage-tracker

# Your Appwrite collection ID for usage data
VITE_APPWRITE_COLLECTION_ID=usage-data